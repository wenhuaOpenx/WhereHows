# Fetch Druid dataset metadata on Holdem
job.class=metadata.etl.dataset.druid.DruidMetadataEtl
job.cron.expr=0 0/5 * * * ?
job.timeout=12000
#job.cmd.params=
#job.disabled=1
job.ref.id=31

druid.host.url=[your druid host url]
druid.datasource.metadata.csv.file=/var/tmp/druid_schema.csv
druid.field.metadata.csv.file=/var/tmp/druid_field.csv
